URxvt*background: 0
URxvt*foreground: 7
URxvt*saveLines: 12000
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt*scrollBar: false
URxvt*scrollstyle: rxvt
URxvt*perl-ext-common: default,matcher,-confirm-paste
URxvt*urlLauncher: x-www-browser
URxvt*matcher.button: 1
URxvt*colorUL: 4
URxvt*pointerBlank: true
URxvt*pointerBlankDelay: 1000000
URxvt*visualBell: true

URxvt*font: xft:IBM Plex Mono:style=light:size=9, xft:EmojiSymbols, xft:Symbola

! orange cursor
URxvt*cursorColor: #f29112

! black
*color0:      #242424
*color8:      #444444
! red
*color1:      #b85149
*color9:      #e5786d
! green
*color2:      #92a65e
*color10:     #95e454
! yellow
*color3:      #ccaa8f
*color11:     #edc4a3
! blue
*color4:      #5b98c2
*color12:     #8ac6f2
! magenta
*color5:      #64619a
*color13:     #a6a1de
! cyan
*color6:      #3f9f9e
*color14:     #70cecc
! white
*color7:      #f6f3e8
*color15:     #ffffff
